0.0.0.0 {
  ## movie
  jwt /movie
  rewrite /movie/.* /movie/SampleVideo_1280x720_2mb.mp4

  ## Backend API
  jwt /json
  proxy /json backend:8000 {
    without /json
  }

  ## auth
  proxy /login auth:8080
  proxy /user auth_dummy:3000 ## https://github.com/nuxt-community/auth-module/issues/213
}

